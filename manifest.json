{
  "manifest_version": 2,

  "name": "RView",
  "description": "Extension to enhance the reddit browsing experience",
  "version":  "1.0.0.1",
  "author": "Jordan Kravitz",

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "content_scripts": [
    {
     "matches": ["*://*.reddit.com/*"],
      "css": ["rview.css"],
      "js": [ "rviewlogic.js", "jquery.min.js"],
      "run_at": "document_start"
    }
  ]

  
}
