{
  "manifest_version": 2,

  "name": "RView",
  "description": "Extension to enhance the reddit browsing experience",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "content_scripts": [
    {
     "matches": ["*://*.reddit.com/*"],/*look into international domains*/
      "css": ["rview.css"],
      "js": [ "rviewlogic.js","jquery.min.js"],
      "run_at": "document_start"
    }
  ]


  
}
